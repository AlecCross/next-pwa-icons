{
  "short_name": "PWA-Icons",
  "name": "PWA-Icons App",
  "description": "A simple PWA for storing and converting icons.",
  "version": "1.0",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "icons": [
    {
      "src": "/icon-192.webp?v=2",
      "sizes": "192x192",
      "type": "image/webp"
    },
    {
      "src": "/icon-512.webp?v=2",
      "sizes": "512x512",
      "type": "image/webp"
    },
    {
      "src": "/icon-16.ico?v=2",
      "sizes": "16x16",
      "type": "image/x-icon"
    },
    {
      "src": "/icon-32.ico?v=2",
      "sizes": "32x32",
      "type": "image/x-icon"
    }
  ],
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "image/webp": [".webp"],
        "image/png": [".png"]
      }
    }
  ]
}
