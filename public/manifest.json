{
  "short_name": "PWA-Icons",
  "name": "PWA-Icons App",
  "description": "A simple PWA for storing and converting icons.",
  "version": "1.0",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "icons": [
    {
      "src": "/icon-16.webp?v=2",
      "sizes": "16x16",
      "type": "image/webp"
    },
    {
      "src": "/icon-32.webp?v=2",
      "sizes": "32x32",
      "type": "image/webp"
    },
    {
      "src": "/icon-192.webp?v=2",
      "sizes": "192x192",
      "type": "image/webp"
    },
    {
      "src": "/icon-512.webp?v=2",
      "sizes": "512x512",
      "type": "image/webp"
    }
  ],
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "image/webp": [".webp"],
        "image/png": [".png"]
      }
    }
  ],
    "share_target": {
      "action": "/share",
      "method": "POST",
      "enctype": "multipart/form-data",
      "params": {
        "files": [
          {
            "name": "file",
            "accept": ["image/webp", "image/png", "image/jpeg"]
          }
        ]
      }
    }
  }